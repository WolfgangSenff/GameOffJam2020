; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "FolderListResource",
"language": "GDScript",
"path": "res://resources/FolderListResource.gd"
}, {
"base": "Node2D",
"class": "Weapon",
"language": "GDScript",
"path": "res://scenes/Weapon.gd"
} ]
_global_script_class_icons={
"FolderListResource": "",
"Weapon": ""
}

[application]

config/name="GameOff2020"
run/main_scene="res://scenes/World.tscn"
config/icon="res://icon.png"

[autoload]

Projectiles="*res://globals/Projectiles.gd"

[debug]

gdscript/warnings/enable=false

[display]

window/size/width=256
window/size/height=160
window/size/test_width=1024
window/size/test_height=640
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "Navigator2D" )

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

